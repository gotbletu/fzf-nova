# {{{ main m3u only
refresh_main() {
wget -c -P "$mydir" -O "$mydir"/iptvorg-main-safe-only-all.m3u 'https://iptv-org.github.io/iptv/index.m3u'
wget -c -P "$mydir" -O "$mydir"/freetuxtv-all.m3u 'https://database.freetuxtv.net/WebStreamExport/index?format=m3u&type=1&status=2&isp=all'
wget -c -P "$mydir" -O "$mydir"/freetv-all.m3u 'https://raw.githubusercontent.com/Free-TV/IPTV/master/playlist.m3u8'
