convert2csv() {
  cd "$mydir" || exit
  echo "Please wait... converting m3u to csv"
  for file in *.m3u;
  do
    m3u2csv "$file" "${file%.*}.csv"
  done
}

export FZF_DEFAULT_OPTS="-e -i --info=default --layout=reverse --scroll-off=5 --tiebreak=index --bind 'home:first,end:last,tab:down,shift-tab:up,ctrl-k:preview-page-up,ctrl-j:preview-page-down,ctrl-y:preview-up,ctrl-e:preview-down,ctrl-/:change-preview-window(hidden|)'"

mylist() {
cat <<EOF
channels| -- watch iptv channels
update-main| -- wget main list by all provider [recommended]
update-freetv| -- wget all by freetv provider
update-freetuxtv| -- wget all by freetuxtv provider
update-iptvorg| -- wget all by iptvorg provider (long wait time)
update-everything| -- wget from all provider (long wait time)
EOF
}
selected="$(mylist | column -s '|' -t | fzf --prompt="ipwebtv: " | cut -d ' ' -f1)"
[ -z "$selected" ] && exit
case $selected in
  update-everything)
    mkdir -p "$mydir"
    refresh_freetv
    refresh_freetuxtv
    refresh_iptvorg
    convert2csv
    ;;
  update-main)
    mkdir -p "$mydir"
    refresh_main
    convert2csv
    ;;
  update-iptvorg)
    mkdir -p "$mydir"
    refresh_iptvorg
    convert2csv
    ;;
  update-freetv)
    mkdir -p "$mydir"
    refresh_freetv
    convert2csv
    ;;
  update-freetuxtv)
    mkdir -p "$mydir"
    refresh_freetuxtv
    convert2csv
    ;;
  channels)
    selected="$( find -L "$mydir" -name '*.csv' | sort \
      | fzf -d / --with-nth -1 --prompt="ipwebtv >>> pick a list: " --preview 'cat {}' \
      )"
    [ -z "$selected" ] && exit
    name=$(basename "$selected" | cut -d. -f1)
    # setsid detach mpv, set channel title, always on top, bottom right corner 
    # shellcheck disable=SC2016
    awk -F'\t' '/http/ && !/EXTVLCOPT/ {print $1 "☺️#" $2 "☺️#" $NF}' "$selected" | column -t -s '☺️' \
      | fzf -d '#' --with-nth ..2 --no-clear --prompt="ipwebtv ($name) >>> pick a channel: " \
      --bind 'home:first,end:last,tab:down,shift-tab:up,enter:execute(setsid mpv \
      --title="$(echo {} | cut -d\# -f1 | xargs)" --ontop --no-border --no-focus-on-open --force-window \
      --autofit=500x280 --geometry=-15-110 "$(echo {} | cut -d\# -f3-)" >/dev/null 2>&1 & disown ; sleep 0.2 \
      )'
    ;;
esac

