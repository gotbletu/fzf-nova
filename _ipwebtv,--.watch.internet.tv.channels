wget -c -P "$mydir" -O "$mydir"/freetv-zz-news-en.m3u 'https://raw.githubusercontent.com/Free-TV/IPTV/master/playlists/playlist_zz_news_en.m3u8'
wget -c -P "$mydir" -O "$mydir"/freetv-zz-news-es.m3u 'https://raw.githubusercontent.com/Free-TV/IPTV/master/playlists/playlist_zz_news_es.m3u8'
wget -c -P "$mydir" -O "$mydir"/freetv-zz-vod-it.m3u 'https://raw.githubusercontent.com/Free-TV/IPTV/master/playlists/playlist_zz_vod_it.m3u8'
}
